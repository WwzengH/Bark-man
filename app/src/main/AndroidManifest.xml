<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.superman">

    <application
       android:name=".app.Myapp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.start.personal.PersonalActivity"/>
        <activity android:name=".ui.start.regard.RegardActivity"/>
        <activity android:name=".ui.start.setting.SetActivity"/>
        <activity android:name=".ui.start.login.LoginActivity"/>
        <activity android:name=".ui.start.StartActivity"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>